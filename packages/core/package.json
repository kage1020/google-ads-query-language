{
  "name": "@gaql/core",
  "version": "0.1.0",
  "description": "Core validation and schema logic for Google Ads Query Language (GAQL)",
  "license": "MIT",
  "author": {
    "name": "kage1020"
  },
  "homepage": "https://github.com/kage1020/google-ads-query-language#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/kage1020/google-ads-query-language.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/kage1020/google-ads-query-language/issues"
  },
  "keywords": [
    "google-ads",
    "gaql",
    "google-ads-query-language",
    "validation",
    "schema",
    "parser",
    "typescript",
    "google-ads-api"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schema": {
      "import": "./dist/schema.js",
      "types": "./dist/schema.d.ts"
    },
    "./validator": {
      "import": "./dist/validator.js",
      "types": "./dist/validator.d.ts"
    },
    "./parser": {
      "import": "./dist/parser.js",
      "types": "./dist/parser.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "node scripts/extract.js",
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "lint": "biome check .",
    "format": "biome format --write .",
    "pretest": "node scripts/extract.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "extract": "node scripts/extract.js",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.3"
  },
  "dependencies": {
    "google-ads-api-v19": "npm:google-ads-api@^19.0.2",
    "google-ads-api-v20": "npm:google-ads-api@^20.0.1",
    "google-ads-api-v21": "npm:google-ads-api@^21.0.1"
  }
}
